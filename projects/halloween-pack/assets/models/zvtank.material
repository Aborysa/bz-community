import * from "BZBase.material"

material zvtank00 : BZBase
{
	set_texture_alias DiffuseMap zvtank_D.dds
	set_texture_alias NormalMap svtank_N.dds
	set_texture_alias SpecularMap svtank_S.dds
	set_texture_alias EmissiveMap zvtank_E.dds
	
	set $diffuse "1 1 1"
	set $ambient "1 1 1"
	set $specular ".7 .7 .7"
	set $shininess "127"
}
