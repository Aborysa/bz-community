import * from "BZBase.material"

material zvhraz00 : BZBase
{
	set_texture_alias DiffuseMap zvhraz_D.dds
	set_texture_alias NormalMap svhraz_N.dds
	set_texture_alias SpecularMap svhraz_S.dds
	set_texture_alias EmissiveMap zvhraz_E.dds
	
	set $diffuse "1 1 1"
	set $ambient "1 1 1"
	set $specular ".7 .7 .7"
	set $shininess "127"
}




